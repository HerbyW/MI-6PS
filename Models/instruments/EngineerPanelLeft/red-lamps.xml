<!-- Pumpensteuerung green-->
	<animation>
	  <type>material</type>
	  <object-name>_c_2.001</object-name>
	  <emission>
	    <factor-prop>controls/switches/gauge-light</factor-prop>
	    <red>   0.35 </red>
	    <green> 0.35 </green>
	    <blue>  0.35 </blue>
	  </emission>
	</animation>
	<!--Brightness-->
	<animation>
	  <type>material</type>
	  <object-name>_d_2.001</object-name>
	  <emission>
	    <factor-prop>controls/switches/gauge-light</factor-prop>
	    <red>   1 </red>
	    <green> 1 </green>
	    <blue>  1 </blue>
	  </emission>
	</animation>
	
	<!--Set-->
	<animation>
	  <type>select</type>
	  <object-name>_d_2.001</object-name>
	  <condition>
	    <or>
