	      <property>controls/switches/Lampentest</property>    
	      <value>1</value>
	    </equals>
	    <equals>
	      <property>controls/switches/Pumpensteuerung</property>    
	      <value>1</value>
	    </equals>
	    </or>
	  </condition>
	</animation>
	
