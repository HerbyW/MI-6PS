<PropertyList>
	<path>AGK-47.ac</path>
	
	<animation>
	  <name>AGK-47</name>
	  
	  <object-name>Artificialhorizon</object-name>
	  <object-name>AI_scala</object-name>
	  <object-name>AI_ring</object-name>
	  <object-name>AI_needle</object-name>
	  <object-name>AI_horizon</object-name>
	  <object-name>AI_Ball</object-name>
	</animation>
	
	<animation>
	  <type>material</type>
	  <object-name>AI_Ball</object-name>
	  <emission>
	    <factor-prop>controls/switches/gauge-light</factor-prop>
	    <red>0.25</red>
	    <green>0.25</green>
	    <blue>0.25</blue>
	  </emission>
	</animation>
	

	<animation>
		<type>range</type>
		<object-name>AGK-47</object-name>
		<min-m>0</min-m>
		<max-m>7</max-m>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>AI_Ball</object-name>
		<object-name>AI_needle</object-name>
		<property>orientation/roll-deg</property>
		<factor>-1</factor>
		<center>
			<x-m>-6.092</x-m>
			<y-m>-0.512</y-m>
			<z-m>3.036</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>AI_Ball</object-name>
		<object-name>AI_needle</object-name>
		<object-name>AI_scala</object-name>
		<property>orientation/pitch-deg</property>
		<factor>-1</factor>
		<center>
			<x-m>-6.092</x-m>
			<y-m>-0.512</y-m>
			<z-m>3.036</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	
</PropertyList>

