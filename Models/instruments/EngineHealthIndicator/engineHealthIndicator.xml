<PropertyList>
	<path>EngineHealthIndicator.ac</path>
	
	<animation>
		<name>EngineHealthIndicator</name>
		<object-name>EngineHealthIndicators</object-name>
		<object-name>leftCoolantPressure</object-name>
		<object-name>leftOilTemperature</object-name>
		<object-name>leftOilPressure</object-name>
		<object-name>rightCoolantPressure</object-name>
		<object-name>rightOilTemperature</object-name>
		<object-name>rightOilPressure</object-name>
	</animation>
	
	<animation>
		<type>range</type>
		<object-name>EngineHealthIndicator</object-name>
		<min-m>0</min-m>
		<max-m>7</max-m>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>leftOilTemperature</object-name>
		<property>/rotors/main/rpm</property>
		<interpolation>
			<entry>
				<ind>-50</ind>
				<dep>-67.5</dep>
			</entry>
			<entry>
				<ind>80</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>400</ind>
				<dep>67.5</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>-4.73665</x-m>
			<y-m>-0.69125</y-m>
			<z-m>3.14314</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>leftOilPressure</object-name>
		<property>/rotors/main/rpm</property>
		<factor> 0.0703 </factor>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>67.5</dep>
			</entry>
			<entry>
				<ind>4.0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>880.0</ind>
				<dep>-67.5</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>-4.73665</x-m>
			<y-m>-0.73158</y-m>
			<z-m>3.14314</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>rightOilTemperature</object-name>
		<property>/rotors/main/rpm</property>
		<interpolation>
			<entry>
				<ind>-50</ind>
				<dep>-67.5</dep>
			</entry>
			<entry>
				<ind>80</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>400</ind>
				<dep>67.5</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>-4.73665</x-m>
			<y-m>-0.50800</y-m>
			<z-m>3.14314</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>rightOilPressure</object-name>
		<property>/rotors/main/rpm</property>
		<factor> 0.0703 </factor>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>67.5</dep>
			</entry>
			<entry>
				<ind>4.0</ind>
				<dep>0</dep>
