<?xml version="1.0" encoding="UTF-8" ?>

<!--
    Instrument from IL-76TD made by:CAXAP
-->

<PropertyList>
 <path>speed_ind.ac</path>

  <animation>
     <object-name>kus</object-name>
     <object-name>group</object-name>
     <object-name>kus_rect2</object-name>
     <object-name>kus_rect1</object-name>    
  </animation>

  <animation>
    <type>material</type>
     <object-name>kus</object-name>
     <object-name>group</object-name>
     <object-name>kus_rect2</object-name>
     <object-name>kus_rect1</object-name>
    <emission>
      <factor-prop>controls/switches/gauge-light</factor-prop>
      <red>   0.35 </red>
      <green> 0.35 </green>
      <blue>  0.35 </blue>
    </emission>
  </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>kus_rect1</object-name>
   
       <property>velocities/airspeed-kt</property>
         
   <interpolation>
      <entry><ind>   -20.00 </ind><dep>   6 </dep></entry>
      <entry><ind> 0</ind><dep>  24</dep></entry>
      <entry><ind> 325.00 </ind><dep> 354 </dep></entry>
    </interpolation>
  
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 <animation>
  <type>rotate</type>
  <object-name>kus_rect2</object-name>
  <property>velocities/groundspeed-kt</property>
  <interpolation>
   <entry><ind>  -20.0</ind><dep>  0</dep></entry>   
   <entry><ind> 0</ind><dep>  20</dep></entry>   
   <entry><ind> 350</ind><dep> 340</dep></entry>
   

  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
</PropertyList>

