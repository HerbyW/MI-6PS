<PropertyList>
  
  <path>rampe.ac</path>
  
<animation>
    <type>rotate</type>        
    <property>sim/multiplay/generic/float[4]</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep> 0 </dep></entry>
      <entry><ind> 0.70 </ind><dep> 0 </dep></entry>
      <entry><ind> 1 </ind><dep>  -31 </dep></entry>
    </interpolation>
    <center>
      <x-m>8.383</x-m>
      <y-m>0</y-m>
      <z-m>1.344</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>-1</y>
      <z>0</z>
    </axis>
  </animation> 
  
