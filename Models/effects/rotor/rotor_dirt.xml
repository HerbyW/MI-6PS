<?xml version="1.0"?><!--Copyright (C) Herbert Wagner 2015-2016modified from Marc Kraus piaseki helicopter--><PropertyList><!-- FRONT ROTOR --><animation>	<name>dirt_front</name>	<object-name>main</object-name>	</animation><animation>		<type>rotate</type>		<object-name>dirt_front</object-name>		<property>orientation/pitch-deg</property>		<axis>			<x>0</x>			<y>-1</y>			<z>0</z>		</axis>	</animation>	<animation>		<type>rotate</type>		<object-name>dirt_front</object-name>		<property>orientation/roll-deg</property>		<axis>			<x>1</x>			<y>0</y>			<z>0</z>		</axis>	</animation>	<animation>		<type>translate</type>		<object-name>dirt_front</object-name>		<property>sim/multiplay/generic/float[12]</property>		<axis>			<x>0</x>			<y>0</y>			<z>-1</z>		</axis>	</animation>		<!-- DIRT - move particle system with rotor tilt -->	<animation>		<object-name>dirt_front</object-name>               <property>sim/multiplay/generic/float[5]</property>		<factor>5</factor>		<center>			<x-m>3.52</x-m>			<y-m>0.0</y-m>			<z-m>5.0</z-m>		</center>		<axis>			<x>0</x>			<y>-1</y>			<z>0</z>		</axis>	</animation>					<!-- FRONT ROTOR - Low GROUND rotor DIRT - particle system controled by engines-->	<particlesystem>		<name>main</name>		<texture>smoke_dirt.png</texture>		<emissive>false</emissive>		<lighting>false</lighting>		<offsets>			<x-m>0.0 </x-m>			<y-m>0.0</y-m>			<z-m>-0.60</z-m>			<roll-deg>90</roll-deg>		</offsets>		<attach>world</attach>		<placer>			<type>point</type>		</placer>		<condition>			<and>				<not>					<property>sim/crashed</property>				</not>				<less-than>					<property>sim/multiplay/generic/float[12]</property>					<value>30</value>				</less-than>				<greater-than>					<property>rotors/main/rpm</property>					<value>50</value>				</greater-than>			</and>		</condition>		<shooter>			<theta-min-deg>0</theta-min-deg>			<theta-max-deg>360</theta-max-deg>			<phi-min-deg>-0.5</phi-min-deg>			<phi-max-deg>3.2</phi-max-deg>			<speed-mps>				<property>sim/multiplay/generic/float[14]</property>				<factor>1</factor>				<interpolation>					<entry><ind>    0.0 </ind><dep> 10.00 </dep></entry>					<entry><ind>  6000.0 </ind><dep> 20.00 </dep></entry>					<entry><ind> 40000.0 </ind><dep> 30.00 </dep></entry>				</interpolation>				<spread>50</spread>			</speed-mps>			<rotation-speed>				<x-max-deg-sec>15</x-max-deg-sec>				<x-min-deg-sec>-15</x-min-deg-sec>				<y-max-deg-sec>15</y-max-deg-sec>				<y-min-deg-sec>-15</y-min-deg-sec>				<z-max-deg-sec>360</z-max-deg-sec>				<z-min-deg-sec>-360</z-min-deg-sec>			</rotation-speed>		</shooter>		<counter>			<particles-per-sec>				<property>sim/multiplay/generic/float[12]</property>				<interpolation>					<entry><ind>  0.0 </ind><dep> 40.00 </dep></entry>					<entry><ind> 15.0 </ind><dep> 10.00 </dep></entry>					<entry><ind> 30.0 </ind><dep>  0.00 </dep></entry>				</interpolation>			</particles-per-sec>		</counter>		<align>billboard</align>		<particle>			<start>				<color>					<red>						<value>0.95</value>					</red>					<green>						<value>0.95</value>					</green>					<blue>						<value>0.95</value>					</blue>					<alpha><value>0.12</value></alpha>				</color>				<size>					<value>0.5</value>				</size>			</start>			<end>				<color>					<red>						<value>1</value>					</red>					<green>						<value>1</value>					</green>					<blue>						<value>1</value>					</blue>					<alpha><value>0.3</value></alpha>				</color>								<size>					<value>11.5</value>				</size>			</end>			<life-sec>				<value>3</value>			</life-sec>			<mass-kg>0.25</mass-kg>			<radius-m>0.045</radius-m>		</particle>		<program>			<fluid>air</fluid>			<gravity>false</gravity>			<wind>true</wind>		</program>	</particlesystem>		</PropertyList>